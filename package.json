{
  "name": "poke-markt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel sass-dev vite",
    "build-ts": "tsc -b",
    "vite-build": "vite build",
    "build": "npm-run-all sass-prod build-ts vite-build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sass-dev": "sass --watch --update --style=expanded src/assets/scss:src/assets/css",
    "sass-prod": "sass --no-source-map --style=compressed src/assets/scss:src/assets/css",
    "vite": "vite",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "eslint-plugin-perfectionist": "^4.9.0",
    "prettier-plugin-ejs": "^1.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "sass": "^1.85.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
